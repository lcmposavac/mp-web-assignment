{
  "name": "ask2ai-web-assigment",
  "private": true,
  "_moduleAliases": {
    "#root": "./"
  },
  "scripts": {
    "dev": "PORT=${PORT:-3000} API_ENDPOINT=${API_ENDPOINT:-https://swapi-graphql.eskerda.vercel.app/} next dev",
    "codegen": "relay-compiler relay.config.json",
    "build": "next build && tsc --project tsconfig.server.json",
    "typecheck": "tsc --noEmit",
    "test:gql": "npm run download-schema && npm run codegen:prod && npm run typecheck",
    "start": "NODE_ENV=production node -r module-alias/register dist/index.js",
    "start:watch": "nodemon --watch 'src/**/*.tsx' --watch 'pages/**/*.tsx' --exec \"next build\"",
    "start:dev": "concurrently \"npm:dev\" \"npm:codegen:watch\" \"npm:dev:server\"",
    "codegen:watch": "nodemon --watch ./schema.graphql --exec 'relay-compiler --watch relay.config.json'",
    "lint": "eslint ./src --ext .ts,.tsx",
    "next:lint": "next lint"
  },
  "lint-staged": {
    "*.tsx": "eslint --fix"
  },
  "dependencies": {
    "@typescript-eslint/eslint-plugin": "^5.26.0",
    "@visx/stats": "^3.5.0",
    "@visx/visx": "^3.5.1",
    "d3": "^7.8.5",
    "d3-svg-legend": "^2.25.6",
    "express": "^4.17.2",
    "framer-motion": "^11.0.5",
    "graphql": "^15.8.0",
    "module-alias": "^2.2.2",
    "moment": "^2.29.4",
    "morgan": "^1.10.0",
    "next": "^13.4.4",
    "nprogress": "^0.2.0",
    "react": "^18.1.0",
    "react-dom": "^18.1.0",
    "react-hot-toast": "^2.2.0",
    "react-icons": "^4.12.0",
    "react-infinite-scroller": "^1.2.6",
    "react-relay": "^16.0.0",
    "relay-connection-handler-plus": "^0.1.2",
    "relay-runtime": "^16.0.0",
    "sass": "^1.49.7",
    "stoppable": "^1.1.0",
    "styled-components": "^5.3.3",
    "ts-is-present": "^1.2.2",
    "use-onclickoutside": "^0.4.0"
  },
  "devDependencies": {
    "@babel/core": "^7.16.7",
    "@graphql-codegen/cli": "^1.21.8",
    "@graphql-inspector/cli": "^2.9.0",
    "@types/d3": "^7.4.3",
    "@types/express": "^4.17.13",
    "@types/morgan": "^1.9.3",
    "@types/node": "16.11.11",
    "@types/react": "^18.0.9",
    "@types/react-dom": "^18.0.4",
    "@types/react-infinite-scroller": "^1.2.5",
    "@types/react-relay": "^16.0.4",
    "@types/relay-runtime": "^14.1.19",
    "@types/stoppable": "^1.1.1",
    "@types/styled-components": "^5.1.21",
    "autoprefixer": "^10.4.2",
    "babel-plugin-relay": "^13.0.1",
    "concurrently": "^7.0.0",
    "eslint": "7",
    "eslint-config-next": "^13.4.4",
    "husky": "^8.0.1",
    "lint-staged": "^13.0.2",
    "nodemon": "^2.0.15",
    "postcss": "^8.4.5",
    "relay-compiler": "^14.1.0",
    "relay-compiler-language-typescript": "^15.0.1",
    "tailwindcss": "^3.1.8",
    "typescript": "4.5.2"
  }
}
